pipeline {
  agent any
  stages {
    stage('Checkout code') {
      steps {
        echo 'Checkout code'
      }
    }

    stage('Build') {
      steps {
        echo 'Build'
      }
    }

    stage('Test') {
      steps {
        echo 'Test'
      }
    }

    stage('push to Docker-Hub') {
      steps {
        echo 'push to Docker-Hub'
      }
    }

  }
}